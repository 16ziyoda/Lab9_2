#include <iostream>
#include <vector>
using namespace std;
int main() {
    int n;
    cin>>n;
    vector<int> a(n);
    for (int i = 0; i < n; ++i){
        cin >> a[i];
        if(a[i]%2==0){
            a[i]=a[i]/2;
        }
        else{ 
            a[i]=a[i]*2;
        }
    } 
    for (int i = n - 1; i >= 0; --i) {
        cout << a[i]<<" ";
    }
    return 0;
}
