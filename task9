#include <iostream>
#include <vector>
using namespace std;

int main() {
    int r, c;
    cin >> r >> c;

    vector<vector<int>> A(r, vector<int>(c));
    vector<vector<int>> B(r, vector<int>(c));
    vector<vector<int>> S(r, vector<int>(c));

    for (int i = 0; i < r; ++i)
        for (int j = 0; j < c; ++j)
            cin >> A[i][j];

    for (int i = 0; i < r; ++i)
        for (int j = 0; j < c; ++j)
            cin >> B[i][j];

    for (int i = 0; i < r; ++i)
        for (int j = 0; j < c; ++j)
            S[i][j] = A[i][j] + B[i][j];

    for (int i = 0; i < r; ++i) {
        for (int j = 0; j < c; ++j) {
            if (j) cout << ' ';
            cout << S[i][j];
        }
        cout << '\n';
    }
    return 0;
}
